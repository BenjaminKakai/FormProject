<h1>Cat Facts</h1>
<table>
  <thead>
    <tr>
      <th>Fact</th>
      <th>Verified</th>
      <th>Created On</th>
    </tr>
  </thead>
  <tbody>
    <% @facts.each do |fact| %>
      <tr>
        <td><%= fact['text'] %></td>
        <td><%= fact['status']['verified'] ? 'True' : 'False' %></td>
        <td><%= Time.parse(fact['createdAt']).strftime('%d-%m-%Y %H:%M:%S') %></td>
      </tr>
    <% end %>
  </tbody>
</table>
